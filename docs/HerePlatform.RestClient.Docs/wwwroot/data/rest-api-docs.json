    {
      "interfaceName": "IIsolineService",
      "description": "Calculate reachable areas (isochrones and isodistances) via the HERE Isoline Routing API v8.",
      "methods": [
        {
          "name": "CalculateIsolineAsync",
          "description": "Compute one or more isolines from a center point within given range values.",
          "returnType": "IsolineResult",
          "parameters": [
            {
              "name": "request",
              "type": "IsolineRequest",
              "required": true,
              "description": "Request parameters for isoline (isochrone/isodistance) calculation.",
              "properties": [
                {
                  "name": "Center",
                  "type": "LatLngLiteral",
                  "required": true,
                  "description": "Center position for the isoline calculation.",
                  "properties": []
                },
                {
                  "name": "Ranges",
                  "type": "List\u003Cint\u003E?",
                  "required": false,
                  "description": "Range values (seconds for time, meters for distance).",
                  "default": "null"
                },
                {
                  "name": "RangeType",
                  "type": "IsolineRangeType",
                  "required": false,
                  "description": "Type of range calculation.",
                  "default": "Time"
                },
                {
                  "name": "TransportMode",
                  "type": "TransportMode",
                  "required": false,
                  "description": "Transport mode (default: Car).",
                  "default": "Car"
                },
                {
                  "name": "RoutingMode",
                  "type": "RoutingMode",
                  "required": false,
                  "description": "Routing optimization mode (default: Fast).",
                  "default": "Fast"
                },
                {
                  "name": "Avoid",
                  "type": "RoutingAvoidFeature",
                  "required": false,
                  "description": "Features to avoid.",
                  "default": "None"
                },
                {
                  "name": "DepartureTime",
                  "type": "string?",
                  "required": false,
                  "description": "Optional departure time (ISO 8601 format).",
                  "default": "null"
                }
              ]
            }
          ]
        }
      ]
    },
    {
