    {
      "interfaceName": "IRoutingService",
      "description": "Route calculation via the HERE Routing API v8. Supports car, truck, pedestrian, bicycle, scooter, and EV routing.",
      "methods": [
        {
          "name": "CalculateRouteAsync",
          "description": "Calculate a route between an origin and destination, with optional via waypoints, avoidance features, truck, and EV parameters.",
          "returnType": "RoutingResult",
          "parameters": [
            {
              "name": "request",
              "type": "RoutingRequest",
              "required": true,
              "description": "Request parameters for route calculation.",
              "properties": [
                {
                  "name": "Origin",
                  "type": "LatLngLiteral",
                  "required": true,
                  "description": "Origin position.",
                  "properties": []
                },
                {
                  "name": "Destination",
                  "type": "LatLngLiteral",
                  "required": true,
                  "description": "Destination position.",
                  "properties": []
                },
                {
                  "name": "Via",
                  "type": "List\u003CLatLngLiteral\u003E?",
                  "required": false,
                  "description": "Optional via waypoints.",
                  "default": "null"
                },
                {
                  "name": "TransportMode",
                  "type": "TransportMode",
                  "required": false,
                  "description": "Transport mode (default: Car).",
                  "default": "Car"
                },
                {
                  "name": "RoutingMode",
                  "type": "RoutingMode",
                  "required": false,
                  "description": "Routing optimization mode (default: Fast).",
                  "default": "Fast"
                },
                {
                  "name": "ReturnPolyline",
                  "type": "bool",
                  "required": false,
                  "description": "Whether to return the route polyline.",
                  "default": "true"
                },
                {
                  "name": "Alternatives",
                  "type": "int",
                  "required": false,
                  "description": "Number of alternative routes to calculate.",
                  "default": "0"
                },
                {
                  "name": "Avoid",
                  "type": "RoutingAvoidFeature",
                  "required": false,
                  "description": "Features to avoid.",
                  "default": "None"
                },
                {
                  "name": "ReturnInstructions",
                  "type": "bool",
                  "required": false,
                  "description": "When true, the response includes turn-by-turn instructions.",
                  "default": "false"
                },
                {
                  "name": "Truck",
                  "type": "TruckOptions?",
                  "required": false,
                  "description": "Truck-specific parameters (only used when TransportMode is Truck).",
                  "default": "null",
                  "properties": [
                    {
                      "name": "Height",
                      "type": "double?",
                      "required": false,
                      "description": "Vehicle height in meters (converted to cm for the API).",
                      "default": "null"
                    },
                    {
                      "name": "Width",
                      "type": "double?",
                      "required": false,
                      "description": "Vehicle width in meters (converted to cm for the API).",
                      "default": "null"
                    },
                    {
                      "name": "Length",
                      "type": "double?",
                      "required": false,
                      "description": "Vehicle length in meters (converted to cm for the API).",
                      "default": "null"
                    },
                    {
                      "name": "GrossWeight",
                      "type": "int?",
                      "required": false,
                      "description": "Gross weight in kilograms.",
                      "default": "null"
                    },
                    {
                      "name": "WeightPerAxle",
                      "type": "int?",
                      "required": false,
                      "description": "Weight per axle in kilograms.",
                      "default": "null"
                    },
                    {
                      "name": "AxleCount",
                      "type": "int?",
                      "required": false,
                      "description": "Number of axles.",
                      "default": "null"
                    },
                    {
                      "name": "TrailerCount",
                      "type": "int?",
                      "required": false,
                      "description": "Number of trailers.",
                      "default": "null"
                    },
                    {
                      "name": "TunnelCategory",
                      "type": "TunnelCategory?",
                      "required": false,
                      "description": "Tunnel category restriction.",
                      "default": "null"
                    },
                    {
                      "name": "HazardousGoods",
                      "type": "HazardousGoods",
                      "required": false,
                      "description": "Hazardous goods flags.",
                      "default": "None"
