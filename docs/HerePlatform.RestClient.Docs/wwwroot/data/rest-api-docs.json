                {
                  "name": "Destinations",
                  "type": "List\u003CLatLngLiteral\u003E",
                  "required": true,
                  "description": "Destination positions."
                },
                {
                  "name": "TransportMode",
                  "type": "TransportMode",
                  "required": false,
                  "description": "Transport mode (default: Car).",
                  "default": "Car"
                },
                {
                  "name": "RoutingMode",
                  "type": "RoutingMode",
                  "required": false,
                  "description": "Routing optimization mode (default: Fast).",
                  "default": "Fast"
                },
                {
                  "name": "DepartureTime",
                  "type": "DateTime?",
                  "required": false,
                  "description": "Optional departure time for time-aware routing.",
                  "default": "null"
