    {
      "interfaceName": "IGeocodingService",
      "description": "Forward and reverse geocoding via the HERE Geocoding \u0026 Search API v1.",
      "methods": [
        {
          "name": "GeocodeAsync",
          "description": "Convert a free-text address or place name into geographic coordinates.",
          "returnType": "GeocodeResult",
          "parameters": [
            {
              "name": "query",
              "type": "string",
              "required": true,
              "description": "Free-text address or place name."
            },
            {
              "name": "options",
              "type": "GeocodeOptions?",
              "required": false,
              "description": "Optional geocoding parameters.",
              "properties": [
                {
                  "name": "Lang",
                  "type": "string?",
                  "required": false,
                  "description": "Language for results (e.g. \u0022en\u0022, \u0022de\u0022).",
                  "default": "null"
                },
                {
                  "name": "Limit",
                  "type": "int",
                  "required": false,
                  "description": "Maximum number of results.",
                  "default": "5"
                },
                {
                  "name": "BoundingBox",
                  "type": "string?",
                  "required": false,
                  "description": "Optional bounding box to narrow results (format: \u0022south,west,north,east\u0022).",
                  "default": "null"
                }
              ]
            }
          ]
        },
        {
          "name": "ReverseGeocodeAsync",
          "description": "Convert geographic coordinates into an address.",
          "returnType": "GeocodeResult",
          "parameters": [
            {
              "name": "position",
              "type": "LatLngLiteral",
              "required": true,
              "description": "Geographic coordinates (lat, lng).",
              "properties": []
            },
            {
              "name": "options",
              "type": "GeocodeOptions?",
              "required": false,
              "description": "Optional geocoding parameters.",
              "properties": [
                {
                  "name": "Lang",
                  "type": "string?",
                  "required": false,
